        <item name="postSplashScreenTheme">@style/Theme.Kwik</item>
        <item name="android:windowSplashScreenBrandingImage">@drawable/splash_view</item>
        <item name="colorPrimary">@android:color/white</item>
        <item name="colorAccent">@color/colorAccent</item>
